#checa mod_rewrite, default activado funciona con ""-
<IfModule mod_rewrite.c>
    Options -Multiviews    
    RewriteEngine On

    RewriteBase /FrameworkMVC/public

    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f    
    RewriteRule ^(.+)$ /index.php?url=$1 [QSA,L]
</IfModule>